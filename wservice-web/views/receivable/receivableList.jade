extends ../layout/layout-nav

block titleContent
    title #{title}

block content
    div.row
        div.col-md-12
            ul.breadcrumb(style="margin-bottom:8px")
                li
                    a(href="#home")
                        span.glyphicon.glyphicon-home 首页
                li.active 收款列表

    div.row
         div.col-md-11
             div.btn-group(data-toggle="buttons")
                 label.btn.btn-default.active
                     input(type="radio",name="options",autocomplete="off" checked)
                     全部订单
                 label.btn.btn-default
                     input(type="radio",name="options",autocomplete="off" )
                     未完成付款
                 label.btn.btn-default
                     input(type="radio",name="options",autocomplete="off" )
                     已完成付款
         div.col-md-1.text-right
             a.btn.btn-default.glyphicon.glyphicon-search(data-toggle="modal",data-target="#search")

    div.row(style="margin-top:10px;")
        div.col-md-12
            div.table-responsive
                table.table.table-striped.table-hover.table-bordered.table-condsend
                    tfoot
                        tr
                            td(colspan="12")
